{
    "name": "spawnrad/oauth-bundle",
    "type": "symfony-bundle",
    "homepage": "http://github.com/spawnrad/HWIOAuthBundle",
    "license": "MIT",
    "description": "Symfony Bundle HWIOAuthBundle Extension.",
    "keywords": [
        "youtube"
    ],
    "authors": [
        {
            "name": "Alexander",
            "email": "iam.asm89@gmail.com"
        },
        {
            "name": "Joseph Bielawski",
            "email": "stloyd@gmail.com"
        },
        {
            "name": "Geoffrey Bachelet",
            "email": "geoffrey.bachelet@gmail.com"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/hwi/HWIOAuthBundle/contributors"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/framework-bundle": "^5.4 || ^6.4 || ^7.1",
        "symfony/security-bundle": "^5.4 || ^6.4 || ^7.1",
        "symfony/options-resolver": "^5.4 || ^6.4 || ^7.1",
        "symfony/form": "^5.4 || ^6.4 || ^7.1",
        "symfony/yaml": "^5.0|^5.4|^6.0",
        "symfony/twig-bundle": "^5.4 || ^6.4 || ^7.1",
        "psr/http-message": "^1.0",
        "php-http/httplug-bundle": "^1.7 || ^2.1"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/orm": "^2.9",
        "symfony/browser-kit": "^5.4 || ^6.4 || ^7.1",
        "symfony/css-selector": "^5.4 || ^6.4 || ^7.1",
        "symfony/property-access": "^5.4 || ^6.4 || ^7.1",
        "symfony/validator": "^5.4 || ^6.4 || ^7.1",
        "symfony/twig-bundle": "^5.0|^5.4|^6.0",
        "symfony/stopwatch": "^5.4 || ^6.4 || ^7.1",
        "symfony/phpunit-bridge": "^5.4 || ^6.4 || ^7.1",
        "symfony/translation": "^5.4 || ^6.4 || ^7.1",
        "friendsofsymfony/user-bundle": "^2.1",
        "php-http/guzzle7-adapter": "^1.0",
        "phpunit/phpunit": "^9.6.11",
        "friendsofphp/php-cs-fixer": "^3.23",
        "symfony/monolog-bundle": "^3.4"
    },
    "conflict": {
        "symfony/security-bundle": ">=6.0,<6.3.4",
        "twig/twig": "<1.43|>=2.0,<2.13"
    },
    "suggest": {
        "doctrine/doctrine-bundle": "to use Doctrine user provider",
        "friendsofsymfony/user-bundle": "to connect FOSUB with this bundle",
        "php-http/httplug-bundle": "to provide required HTTP client with ease.",
        "symfony/property-access": "to use FOSUB integration with this bundle",
        "symfony/twig-bundle": "to use the Twig hwi_oauth_* functions"
    },
    "autoload": {
        "psr-4": {
            "HWI\\Bundle\\OAuthBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {}
    },
    "minimum-stability": "dev"
}